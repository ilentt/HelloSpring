<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="springHello" class="vn.elca.training.SpringHello"></bean>
<bean id="StrustHello" class="vn.elca.training.StrustHello"></bean>
<bean id="helloService" class="vn.elca.training.HelloService">
  <property name="hello" ref="springHello"></property>
</bean>
</beans>
